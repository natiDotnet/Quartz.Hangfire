﻿﻿<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net10.0</TargetFramework>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
        <LangVersion>latest</LangVersion>
        <PackageId>Quartz.Extensions.Hangfire</PackageId>
        <Title>Quartz.Extensions.Hangfire</Title>
        <Authors />
        <PackageTags>quartz, hangfire, quartz.hangfire</PackageTags>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <PackageProjectUrl>https://github.com/natiDotnet/Quartz.Hangfire</PackageProjectUrl>
        <RepositoryUrl>https://github.com/natiDotnet/Quartz.Hangfire</RepositoryUrl>
        <PackageReadmeFile>README.md</PackageReadmeFile>
    </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="Hangfire.Core" Version="1.8.22" />
      <PackageReference Include="Microsoft.Extensions.DependencyInjection.Abstractions" Version="10.0.0" />
      <PackageReference Include="Quartz" Version="3.15.1" />
      <None Include="README.md" Pack="true" PackagePath=""/>
    </ItemGroup>

</Project>
